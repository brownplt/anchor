#!/bin/bash

set -x -e

mkdir -p build/syntax

cp src/syntax/pyret-tokenizer.js build/syntax

cp -R src/jglr build/syntax

node src/jglr/parser-generator.js src/syntax/pyret-grammar.bnf build/syntax/pyret-grammar.js "./jglr" "jglr/jglr" "pyret-parser"

node build/syntax/pyret-grammar.js build/syntax/pyret-parser.js
